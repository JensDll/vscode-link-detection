{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Example language",
  "scopeName": "text.exlang",
  "patterns": [
    {
      "include": "#type"
    }
  ],
  "repository": {
    "type": {
      "match": "\\b[A-Z_]{2,}\\b",
      "name": "support.type exlang.type"
    }
  }
}
